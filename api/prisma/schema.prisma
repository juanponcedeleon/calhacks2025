  generator client {
    provider = "prisma-client-js"
  }

  datasource db {
    provider = "sqlite"
    url      = "file:./dev.db"
  }

  model Listing {
    id           String   @id 
    name         String
    description  String
    endTime      Float
    minBid       Float
  }

  model Bid {
    id           String   @id 
    name         String
    bid          Float
    endTime      Float
    minBid       Float
  }

  model Cursor {
    id           String   @id 
    eventSeq     String
    txDigest     String
  }
